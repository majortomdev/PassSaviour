<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>VerifyLogin</servlet-name>
        <servlet-class>com.kinahan.connect.VerifyLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateNewUser</servlet-name>
        <servlet-class>com.kinahan.connect.CreateNewUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EncryptIt</servlet-name>
        <servlet-class>com.kinahan.encryption.EncryptIt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenKey</servlet-name>
        <servlet-class>com.kinahan.encryption.GenKey</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>finalAuthentication</servlet-name>
        <servlet-class>com.kinahan.connect.FinalAuthentication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckVaultPhrase</servlet-name>
        <servlet-class>com.kinahan.connect.CheckVaultPhrase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PopulateDisplay</servlet-name>
        <servlet-class>com.kinahan.connect.PopulateDisplay</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VerifyLogin</servlet-name>
        <url-pattern>/VerifyLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateNewUser</servlet-name>
        <url-pattern>/CreateNewUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EncryptIt</servlet-name>
        <url-pattern>/EncryptIt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenKey</servlet-name>
        <url-pattern>/GenKey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>finalAuthentication</servlet-name>
        <url-pattern>/finalAuthentication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckVaultPhrase</servlet-name>
        <url-pattern>/CheckVaultPhrase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PopulateDisplay</servlet-name>
        <url-pattern>/PopulateDisplay</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
